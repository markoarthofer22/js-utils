{
    "name": "@markoarthofer22/js-utils",
    "version": "1.0.0",
    "description": "Collection of js utils written with vanilla js (ts)",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "files": [
        "dist",
        "README.md"
    ],
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/markoarthofer22/js-utils/"
    },
    "keywords": [
        "react-components"
    ],
    "author": "Marko Arthofer",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/markoarthofer22/js-utils/issues"
    },
    "homepage": "https://github.com/markoarthofer22/js-utils#readme",
    "scripts": {
        "tsc": "tsc",
        "build": "rimraf dist && tsc && rollup --config",
        "lint": "eslint src --ext=.js,.ts,.tsx,.jsx",
        "prettier": "prettier --write '**/*.{js,ts,tsx,jsx}'",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "npm run prettier",
            "npm run lint"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^15.0.2",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.9.0",
        "@typescript-eslint/parser": "^4.33.0",
        "danger": "^10.6.6",
        "eslint": "^7.28.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-node": "^4.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^6.0.0",
        "jest": "^27.0.4",
        "lint-staged": "^11.1.2",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.56.3",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "ts-jest": "^27.0.4",
        "typescript": "^4.3.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "npm": ">=6.14.13",
        "node": ">=14.7.0"
    }
}
